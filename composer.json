{
  "name": "zanphp/database",
  "repositories": {
    "packagist": {
      "type": "composer",
      "url": "http://packagist.qima-inc.com"
    }
  },
  "require": {
    "php": ">=5.6",
    "zanphp/coroutine": "dev-master",
    "zanphp/contracts": "dev-master",
    "zanphp/timer": "dev-master",
    "zanphp/exception": "dev-master",
    "zanphp/support": "dev-master"
  },
  "require-dev": {
    "zanphp/connection-pool": "dev-master",
    "zanphp/spi":"dev-master",
    "zanphp/config":"dev-master",
    "zanphp/testing": "dev-master"
  },
  "autoload": {
    "psr-4": {
      "ZanPHP\\Database\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZanPHP\\Database\\Tests\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "config": {
    "preferred-install": "dist",
    "secure-http": false
  },
  "minimum-stability": "dev"
}